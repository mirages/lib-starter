{
  "name": "lib-starter",
  "version": "0.1.0",
  "description": "a library starter based on typescript, rollup, karma and mocha",
  "engines": {
    "node": ">=10.13.0",
    "git": ">=2.13.0"
  },
  "main": "src/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "rollup": "rollup -c",
    "types": "tsc",
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mirages/lib-starter.git"
  },
  "keywords": [
    "lib-starter",
    "typescript",
    "rollup",
    "karma"
  ],
  "author": "mirages",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mirages/lib-starter/issues"
  },
  "homepage": "https://github.com/mirages/lib-starter#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.3",
    "chai": "^4.2.0",
    "commitizen": "^4.2.2",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "3.3.0",
    "husky": "^4.3.0",
    "karma": "^5.2.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-typescript": "^5.2.0",
    "lint-staged": "^10.5.1",
    "mocha": "^8.2.1",
    "prettier": "2.1.2",
    "rollup": "^2.33.1",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "karma start --single-run --browsers ChromeHeadless && lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "dependencies": {}
}
